          MODULE=gnome-python-extras
           MAJOR=2.25
         VERSION=$MAJOR.3
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
      SOURCE_VFY=sha1:e69e5288466c960ed6f985bb10c9969b21ed85a4
