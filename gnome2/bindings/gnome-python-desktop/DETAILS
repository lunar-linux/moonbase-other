          MODULE=gnome-python-desktop
           MAJOR=2.32
         VERSION=$MAJOR.0
          SOURCE=$MODULE-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
      SOURCE_VFY=sha1:dae15fe9fea61ecbde5af513c94de2ff1a66369c
        WEB_SITE=http://www.pygtk.org
         ENTERED=20050319
         UPDATED=20101203
           SHORT="Python bindings for the GNOME desktop"

cat << EOF
GnomePythonDesktop  provides python interfacing modules for some GNOME
libraries part of the GNOME Desktop (gnomeapplet, gnomeprint,
gnomeprint.ui, gtksourceview, wnck, totem.plparser, gtop, nautilusburn,
mediaprofiles, metacity).
EOF
