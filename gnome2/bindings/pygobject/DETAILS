          MODULE=pygobject
         VERSION=2.16.0
           MAJOR=2.16
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR/
      SOURCE_VFY=sha1:bb16032be31b0651160e9a2caf11c1115c69c85a
