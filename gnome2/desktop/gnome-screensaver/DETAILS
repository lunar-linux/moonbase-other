          MODULE=gnome-screensaver
         VERSION=2.20.0
           MAJOR=2.20
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR/
      SOURCE_VFY=sha1:0b99de04caf897642338c61edc12cb7cea27e0a4
