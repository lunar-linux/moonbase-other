          MODULE=gvfs
           MAJOR=1.6
         VERSION=$MAJOR.2
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
      SOURCE_VFY=sha1:
        WEB_SITE=http://www.gnome.org
         ENTERED=20080312
         UPDATED=20100527
           SHORT="Replacement for Gnome-VFS"

cat << EOF
GVFS is designed to supercede Gnome-VFS.
EOF
