         VERSION=$MAJOR.1
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
      SOURCE_VFY=sha1:1a79546133708e18400b42cc1d621ab38962c573
