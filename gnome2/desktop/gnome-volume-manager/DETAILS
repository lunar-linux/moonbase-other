          MODULE=gnome-volume-manager
         VERSION=2.17.0
           MAJOR=2.17
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
      SOURCE_VFY=sha1:2b2f1367823c92fbeb269c10959adca2d3db1918
        WEB_SITE=http://www.gnome.org
         ENTERED=20050316
