          MODULE=gnome-desktop
           MAJOR=2.32
         VERSION=$MAJOR.1
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
      SOURCE_VFY=sha1:95767b6696ebc406b4349b383366b9aff32e39f4
        WEB_SITE=http://www.gnome.org
