          MODULE=gnome-desktop
           MAJOR=2.30
         VERSION=$MAJOR.0
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
      SOURCE_VFY=sha1:b9b2363d9de4fdc7ca999a0fd4769e74d6429364
        WEB_SITE=http://www.gnome.org
