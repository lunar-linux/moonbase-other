        MODULE=orca
       VERSION=2.28.0
         MAJOR=2.28
        SOURCE=$MODULE-$VERSION.tar.bz2
    SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
    SOURCE_VFY=sha1:b39fc9a558fcb4effd7ef36ef85b346b4093458a
       ENTERED=20070315
