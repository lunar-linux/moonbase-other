        MODULE=orca
       VERSION=2.24.3
         MAJOR=2.24
        SOURCE=$MODULE-$VERSION.tar.bz2
    SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
    SOURCE_VFY=sha1:828c93c07b63217d8d4ca47d2a4ae46e9eb3e489
