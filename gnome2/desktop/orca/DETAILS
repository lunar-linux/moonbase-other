        SOURCE=$MODULE-$VERSION.tar.bz2
    SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
    SOURCE_VFY=sha1:f4c387ad4763c6ca83033dc193ab5b80fb2d4cc4
       ENTERED=20070315
