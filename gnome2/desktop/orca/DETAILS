        MODULE=orca
       VERSION=2.26.3
         MAJOR=2.26
        SOURCE=$MODULE-$VERSION.tar.bz2
    SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
    SOURCE_VFY=sha1:e04cf4c92b4ef474dead0594a1a9df89b8e1ade9
       ENTERED=20070315
