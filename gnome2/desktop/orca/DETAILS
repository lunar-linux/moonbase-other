        SOURCE=$MODULE-$VERSION.tar.bz2
    SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
    SOURCE_VFY=sha1:19329deb88224c3e637fa323fa14602b5d4f45f8
       ENTERED=20070315
