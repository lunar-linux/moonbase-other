        MODULE=orca
       VERSION=2.18.0
         MAJOR=2.18
        SOURCE=$MODULE-$VERSION.tar.bz2
    SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
    SOURCE_VFY=sha1:af3b7f3379cafb9db3ea1352a778e51c4f123a6c
