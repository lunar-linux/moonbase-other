        SOURCE=$MODULE-$VERSION.tar.bz2
    SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
    SOURCE_VFY=sha1:110b480ab085cfeb8d86a1c99aaf45d2189d0b16
       ENTERED=20070315
