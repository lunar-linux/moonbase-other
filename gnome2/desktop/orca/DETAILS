        MODULE=orca
       VERSION=2.26.2
         MAJOR=2.26
        SOURCE=$MODULE-$VERSION.tar.bz2
    SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
    SOURCE_VFY=sha1:849ff2d3a8b65ca8c66b2c706d296deec87069f2
       ENTERED=20070315
