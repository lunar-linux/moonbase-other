depends  scrollkeeper
depends  gtk+-2

optional_depends  "libgnomeprintui"  \
                  "--enable-gnome"   \
                  "--disable-gnome"  \
                  "for GNOME widget support"

optional_depends  "libgnomedb"          \
                  "--enable-gnome-db"   \
                  "--disable-gnome-db"  \
                  "for gnome-db widget support"
