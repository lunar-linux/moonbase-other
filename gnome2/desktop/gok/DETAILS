          MODULE=gok
           MAJOR=2.30
         VERSION=$MAJOR.1
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
      SOURCE_VFY=sha1:cc7690863eb3e27687daeff10cee2c49c07f4f30
