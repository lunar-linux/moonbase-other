          MODULE=gnumeric2
             MOD=gnumeric
         VERSION=1.8.3
          SOURCE=$MOD-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MOD-$VERSION
      SOURCE_URL=$GNOME_URL/sources/$MOD/1.8/
      SOURCE_VFY=sha1:5a6585549bd705bf1d0e590981c3ef38173f8c08
        WEB_SITE=http://www.gnumeric.org
