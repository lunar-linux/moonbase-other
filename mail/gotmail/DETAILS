          MODULE=gotmail
         VERSION=0.8.5
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE/
      SOURCE_VFY=sha1:bbe697920d748242d197d33d94a6d76711a696f6
        WEB_SITE=http://gotmail.sourceforge.net/
