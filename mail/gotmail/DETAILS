          MODULE=gotmail
         VERSION=0.8.9
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE/
      SOURCE_VFY=sha1:50e7f65f554302b48f0aba8b0a27cedd7bf1c174
        WEB_SITE=http://gotmail.sourceforge.net/
