          MODULE=mailutils
         VERSION=3.14
          SOURCE=${MODULE}-${VERSION}.tar.xz
      SOURCE_URL=ftp://ftp.gnu.org/pub/gnu/mailutils/
      SOURCE_VFY=sha256:c0c5b38fea8b45a4afcd436487f2a76fd5525092d0378813a6eb5542c2127139
