      SOURCE_URL=http://nice.freedesktop.org/releases/
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
      SOURCE_VFY=sha1:821cbeeb972874855724d8a9cfbaaa3086f62881
