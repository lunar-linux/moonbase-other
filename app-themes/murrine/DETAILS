          MODULE=murrine
           MAJOR=0.98
         VERSION=$MAJOR.1.1
          SOURCE=murrine-$VERSION.tar.bz2
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR/
      SOURCE_VFY=sha1:ffd6226b9941c01696497ec7c881551ea2f150c7
        WEB_SITE=http://www.cimitan.com/murrine/
         ENTERED=20060725
         UPDATED=20110305
           SHORT="Cairo-based GTK+-2 theme engine"
      MAINTAINER=jannis@lunar-linux.org

cat << EOF
Murrine is a GTK+-2 theme engine. "Murrine" is an Italian word
meaning the glass artworks done by Venician glass blowers. The engine
is cairo-based, and it's very fast compared to clearlooks-cairo and
ubuntulooks.
EOF
