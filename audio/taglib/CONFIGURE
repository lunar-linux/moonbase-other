mquery USE_MP4 "Enable MP4 tagging support? (Used by Amarok)" n "-DWITH_MP4=ON" "-DWITH_MP4=OFF"
mquery ENABLE_ITUNE_HACK "Disable workarounds for iTunes bugs?" n "-DNO_ITUNES_HACKS=ON" "-DNO_ITUNES_HACKS=OFF"
