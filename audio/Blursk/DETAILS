         VERSION=1.3
          SOURCE=$MODULE-$VERSION.tar.gz
   SOURCE_URL[0]=http://www.cs.pdx.edu/~kirkenda/blursk/
        WEB_SITE=http://www.cs.pdx.edu/~kirkenda/blursk/
         ENTERED=20020320
