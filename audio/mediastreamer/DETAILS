          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=http://download.savannah.gnu.org/releases-noredirect/linphone/mediastreamer/
      SOURCE_VFY=sha1:eae7032fbbf5d31b3c47a54566d34a8b97e38405
