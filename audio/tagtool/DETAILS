          MODULE=tagtool
         VERSION=0.12.3
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$SFORGE_URL/$MODULE/
      SOURCE_VFY=sha1:e7ed4ebf8f9881cf38b2340b18e95947f658308e
        WEB_SITE=http://pwp.netcabo.pt/paol/tagtool/
