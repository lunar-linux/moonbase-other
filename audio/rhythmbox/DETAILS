           MODULE=rhythmbox
         VERSION=0.8.7
          SOURCE=$MODULE-$VERSION.tar.gz
   SOURCE_URL[0]=$SFORGE_URL/$MODULE/
   SOURCE_URL[1]=$GNOME_URL/sources/$MODULE/0.8/
      SOURCE_VFY=sha1:3632c090e49fddd1fedca46b9420dd2fbdd1b269
