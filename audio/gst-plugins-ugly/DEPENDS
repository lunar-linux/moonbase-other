depends gst-plugins-base

optional_depends a52dec        "-D a52dec=enabled"   "" "for AC-3 support"
optional_depends libdvdread    "-D dvdread=enabled"  "" "for DVD support"
optional_depends libcdio       "-D cdio=enabled"     "" "for CD control support"
optional_depends mpeg2dec      "-D mpeg2dec=enabled" "" "for MPEG-2 video support"
optional_depends x264-snapshot "-D x264=enabled"     "" "for H264 video support"
optional_depends orc           "-D orc=enabled"      "" "for runtime compiler support"
optional_depends amrnb         "-D amrnb=enabled"    "" "for AMR narrowband support"
optional_depends amrwb         "-D amrwbdec=enabled"    "" "for AMR wideband decoder support"
