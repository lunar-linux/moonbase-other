mquery ENBL_CUST "Enable non-Opus modes, e.g. 44.1 kHz & 2^n frames?" y "--enable-custom-modes" "--disable-custom-modes"
mquery ENBL_FAST "Enable fast approximations for floating point?" y "--enable-float-approx" "--disable-float-approx"
mquery ENBL_FUZZ "Enable fuzz causing the encoder to make random decisions?" n "--enable-fuzzing" "--disable-fuzzing"
mquery ENBL_INTRINSICS "Enable intrinsics optimizations for ARM(float) X86(fixed)?" y "--enable-intrinsics" "disable-intrinsics"
