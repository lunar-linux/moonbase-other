          MODULE=amrwb
         VERSION=11.0.0.0
          SOURCE=$MODULE-$VERSION.tar.bz2
         SOURCE2=26204-700.zip
      SOURCE_URL=ftp://ftp.penguin.cz/pub/users/utx/amr/
     SOURCE2_URL=http://www.3gpp.org/ftp/Specs/archive/26_series/26.204
