 SOURCE_URL_FULL=https://github.com/kode54/dumb/archive/refs/tags/$VERSION.tar.gz
      SOURCE_VFY=sha256:99bfac926aeb8d476562303312d9f47fd05b43803050cd889b44da34a9b2a4f9
