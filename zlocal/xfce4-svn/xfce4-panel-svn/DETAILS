   MODULE_PREFIX=${XFCE4_SVN_PREFIX:-/opt/lunar/xfce4-svn/}
          MODULE=xfce4-panel-svn
         VERSION=beta
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=svn+http://svn.xfce.org/svn/xfce/${MODULE//-svn}/trunk/:$MODULE-$VERSION
        WEB_SITE=http://www.xfce.org
         ENTERED=20060407
         UPDATED=`date +%Y%m%d`
           SHORT="Xfce4 panel"

cat << EOF
xfce4-panel-svn -- The panel for the Xfce4 desktop environment. Provides program
lauchers, menus, and several plugins like a clock a pager and a mail checker.
Allows external plugins to be written and installed separately.

EOF
