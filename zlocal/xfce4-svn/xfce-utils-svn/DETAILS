   MODULE_PREFIX=${XFCE4_SVN_PREFIX:-/opt/lunar/xfce4-svn/}
          MODULE=xfce-utils-svn
         VERSION=beta
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=svn+http://svn.xfce.org/svn/xfce/${MODULE//-svn}/trunk/:$MODULE-$VERSION
        WEB_SITE=http://www.xfce.org
         ENTERED=20060407
         UPDATED=`date +%Y%m%d`
           SHORT="Essential utilities for Xfce4"

cat << EOF
xfce-utils-svn -- Several essential utilities and scripts for XFce 4. Provides a
askbar, the about dialog, the run dialog, the user guide and several useful
scripts that are used by other Xfce4 components.

EOF
