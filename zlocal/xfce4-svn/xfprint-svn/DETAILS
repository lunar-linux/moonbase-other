   MODULE_PREFIX=${XFCE4_SVN_PREFIX:-/opt/lunar/xfce4-svn/}
          MODULE=xfprint-svn
         VERSION=beta
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=svn+http://svn.xfce.org/svn/xfce/${MODULE//-svn}/trunk/:$MODULE-$VERSION
        WEB_SITE=http://www.xfce.org
         ENTERED=20060407
         UPDATED=`date +%Y%m%d`
           SHORT="Printer dialog for Xfce4"

cat << EOF
xfprint-svn -- Printer manager and print dialog for the Xfce4 desktop
environment.

EOF
