   MODULE_PREFIX=${XFCE4_SVN_PREFIX:-/opt/lunar/xfce4-svn/}
          MODULE=xfwm4-svn
         VERSION=beta
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=svn+http://svn.xfce.org/svn/xfce/${MODULE//-svn}/trunk/:$MODULE-$VERSION
        WEB_SITE=http://www.xfce.org
         ENTERED=20060407
         UPDATED=`date +%Y%m%d`
           SHORT="Window manager for Xfce4"

cat << EOF
xfwm4-svn -- Window manager for the Xfce4 desktop environement. Strictly follows
the freedesktop standard on window manager hints.

EOF
