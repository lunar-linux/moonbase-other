   MODULE_PREFIX=${XFCE4_SVN_PREFIX:-/opt/lunar/xfce4-svn/}
          MODULE=xfdesktop-svn
         VERSION=beta
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=svn+http://svn.xfce.org/svn/xfce/${MODULE//-svn}/trunk/:$MODULE-$VERSION
        WEB_SITE=http://www.xfce.org
         ENTERED=20060407
         UPDATED=`date +%Y%m%d`
           SHORT="Desktop manager for Xfce4"

cat << EOF
xfdesktop-svn -- Desktop manager for the Xfce4 desktop environment. Handles the
background image settings and provides a mouse menu when you click on the
desktop background.

EOF
