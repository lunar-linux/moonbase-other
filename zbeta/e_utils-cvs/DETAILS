          MODULE=e_utils-cvs
         VERSION=beta
          SOURCE=$MODULE-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/e_utils
      SOURCE_URL=cvs://:pserver:anonymous@cvs.sourceforge.net:/cvsroot/enlightenment:e17/apps/e_utils
        WEB_SITE=http://www.enlightenment.org
         ENTERED=20041230
         UPDATED=`date -u +%Y%m%d`
   MODULE_PREFIX=${E17_PREFIX:-/opt/lunar/enlightenment}
           SHORT="additional e17 utilities"

cat << EOF
Optional utilities and applications for e17.

Currently contains:
 * e_util_eapp_edit - an .eap editor
 * e17setroot       - a commandline background setter
 * emblem           - a GUI background setter
 * entangle         - a simple e17 menu editor
 * exige            - a run application dialog
 * ethemes          - an e17 theme previewer
EOF
