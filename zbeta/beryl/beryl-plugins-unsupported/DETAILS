          MODULE=beryl-plugins-unsupported
         VERSION=git
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=git://git.beryl-project.org/beryl/$MODULE
        WEB_SITE=http://www.beryl-project.org/
         ENTERED=20060925
         UPDATED=`date -u +%Y%m%d`
           SHORT="additional plugins for beryl"

cat<<EOF
This is an additional plugins package for beryl.  The plugins in this
pacakge may not be as stable or feature-complete as the ones found in
beryl-plugins.  To activate, deactivate or tweak plugins, run 
beryl-settings.
EOF
