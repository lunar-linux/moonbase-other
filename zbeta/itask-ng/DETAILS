          MODULE=itask-ng
         VERSION=beta
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=svn+http://itask-module.googlecode.com/svn/trunk/
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION/itask-ng/
      SOURCE_VFY=
        WEB_SITE="http://itask-module.googlecode.com/"
         ENTERED=20080713
         UPDATED=`date -u +%Y%m%d`
           SHORT="An alternate taskbar/launcher for E17."
   MODULE_PREFIX=${E17_PREFIX:-/opt/lunar/enlightenment}

