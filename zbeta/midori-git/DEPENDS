depends  libsexy
depends  webkit
depends  libxml2
depends  libsoup

optional_depends libunique "--enable-unique" "--disable-unique" \
                 "for single instance midori"

optional_depends sqlite  "--enable-sqlite" "--disable-sqlite" \
                 "enable history database support"

optional_depends libidn "--enable-libidn" "--disable-libidn" \
                 "enable international domain name support"

optional_depends gtk-doc "--enable-apidocs" "--disable-apidocs" \
                 "enable creation of apidocs"
