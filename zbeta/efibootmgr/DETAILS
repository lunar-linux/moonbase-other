          MODULE=efibootmgr
         VERSION=0.5.4
          SOURCE=${MODULE}-${VERSION}.tar.gz
      SOURCE_URL=http://linux.dell.com/efibootmgr/
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
      SOURCE_VFY=sha1:eebdb671638f3a7901690df2a8f647e7f8666ed2
        WEB_SITE="http://linux.dell.com/efibootmgr/"
         ENTERED=20110825
         UPDATED=20110825
           SHORT="EFI boot manager"
cat <<EOF
efibootmgr is an EFI boot manager for linux. It allows the EFI boot
order to be modified while linux is running.
EOF
