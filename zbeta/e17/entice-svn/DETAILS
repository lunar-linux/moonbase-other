          MODULE=entice-svn
         VERSION=svn
          SOURCE=$MODULE.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE
      SOURCE_URL=svn+http://svn.enlightenment.org/svn/e/trunk/entice
        WEB_SITE=http://atmos.org/docs/entice/index.html
         ENTERED=20041227
         UPDATED=`date --date="last monday" +%Y%m%d`
   MODULE_PREFIX=${E17_PREFIX:-/opt/lunar/enlightenment}
           SHORT="An e17 image viewer"

cat << EOF
Entice is an image viewer built on the Enlightenment Foundation
Libraries.  It conforms to the Freedesktop Thumbnail Specification,
offers rotation and resizing functionality, as well as a sexy, easy
interface.
EOF
