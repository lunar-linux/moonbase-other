          MODULE=embryo-cvs
         VERSION=beta
          SOURCE=$MODULE-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/embryo
      SOURCE_URL=cvs://:pserver:anonymous@anoncvs.enlightenment.org:/var/cvs/e:e17/libs/embryo
