          MODULE=airconfig-svn
             MOD=airconfig
         VERSION=svn
          SOURCE=$MOD.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MOD
      SOURCE_URL=svn+http://svn.xfce.org/svn/kelnos/$MOD/trunk:$MOD
   MODULE_PREFIX=${XFCE4_PREFIX:-/usr}
        WEB_SITE=http://spuriousinterrupt.org/projects/airconfig
         ENTERED=20071114
         UPDATED=`date --date="last monday" +%Y%m%d`
           SHORT="Xfce wireless network configuration utility"

cat << EOF
Xfce wireless network configuration utility.
Start wpa_supplicant as root with:
wpa_supplicant -B -P /var/run/wpa_supplicant.pid -u
EOF
