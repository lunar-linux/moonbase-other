          MODULE=logilab-common
         VERSION=0.22.1
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION/
      SOURCE_URL=ftp://ftp.logilab.fr/pub/${MODULE//logilab-}/
      SOURCE_VFY=sha1:f33dd10af666ee4c531a2b1a087a587f1f65952b
        WEB_SITE=http://www.logilab.org/projects/${MODULE//logilab-}/
         ENTERED=20070622
         UPDATED=20070622
           SHORT="common low level library required by Logilab projects"

cat << EOF
The package 'logilab.common' contains several modules providing low level
functionalities shared among some python projects developed by Logilab.
EOF

