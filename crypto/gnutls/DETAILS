          MODULE=gnutls
         VERSION=3.1.6
          SOURCE=$MODULE-$VERSION.tar.xz
   SOURCE_URL[0]=$GNU_URL/$MODULE
   SOURCE_URL[1]=ftp://ftp.gnutls.org/gcrypt/$MODULE/v${VERSION::3}
      SOURCE_VFY=sha1:3c4d9fc40305895079676a6ebb01d87da810cd8d
