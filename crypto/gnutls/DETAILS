          MODULE=gnutls
         VERSION=3.2.3
          SOURCE=$MODULE-$VERSION.tar.xz
   SOURCE_URL[0]=$GNU_URL/$MODULE
   SOURCE_URL[1]=ftp://ftp.gnutls.org/gcrypt/$MODULE/v${VERSION%.*}
