          MODULE=showeq
	 VERSION=5.3.0.0
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$SFORGE_URL/seq/
     SOURCE2_URL=$PATCH_URL
      SOURCE_VFY=sha1:efee2bd98808b1f7f63509e6efa6b53b43da2f48
        WEB_SITE=http://seq.sourceforge.net/
