mk_source_dir                                       &&
unpack $SOURCE                                      &&
unpack $SOURCE2                                     &&
cp opengfx-0.3.5/*.grf $SOURCE_DIRECTORY/bin/data/  &&
cp opengfx-0.3.5/*.obg $SOURCE_DIRECTORY/bin/data/  &&
rm -rf opengfx-0.3.5                                &&
cd $SOURCE_DIRECTORY
