mk_source_dir                                       &&
unpack $SOURCE                                      &&
unpack $SOURCE2                                     &&
cp opengfx-0.2.4/*.grf $SOURCE_DIRECTORY/bin/data/  &&
cp opengfx-0.2.4/*.obg $SOURCE_DIRECTORY/bin/data/  &&
rm -rf opengfx-0.2.4                                &&
cd $SOURCE_DIRECTORY
