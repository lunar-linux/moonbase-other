          SOURCE=$MODULE-$VERSION-src.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE/
     SOURCE_VFY=sha1:d3e15d6137503d79ab1d5ac5ddb1d01a20173671
        WEB_SITE=http://stratagus.sourceforge.net/
