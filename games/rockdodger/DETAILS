          MODULE=rockdodger
         VERSION=0.6
          SOURCE=$MODULE-$VERSION.tar.gz
         SOURCE2=$MODULE-$VERSION-data-dirs.patch.bz2
   SOURCE_URL[0]=$SFORGE_URL/spacerocks/
  SOURCE2_URL[0]=$PATCH_URL
        WEB_SITE=http://spacerocks.sourceforge.net
