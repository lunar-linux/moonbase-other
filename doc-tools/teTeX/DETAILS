          MODULE=teTeX
         VERSION=2.0.1
          SOURCE=tetex-src-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/tetex-src-$VERSION
   SOURCE_URL[0]=ftp://cam.ctan.org/tex-archive/systems/unix/teTeX/current/distrib/$SOURCE
   SOURCE_URL[1]=ftp://dante.ctan.org/tex-archive/systems/unix/teTeX/current/distrib/$SOURCE
