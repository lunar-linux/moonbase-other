          SOURCE=$MODULE-$VERSION.tgz
      SOURCE_URL=$SFORGE_URL/$MODULE/
      SOURCE_VFY=sha1:d2b327357afc94a94701d260936b037abf4212e8
         WEBSITE=http://ipw2200.sourceforge.net
