	  MODULE=ipw2100
	 VERSION=1.1.0
	  SOURCE=$MODULE-$VERSION.tgz
	 SOURCE2=$MODULE-$VERSION-multicast.patch
      SOURCE_URL=$SFORGE_URL/$MODULE/
     SOURCE2_URL=$PATCH_URL/
