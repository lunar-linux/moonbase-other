          MODULE=orage
             MOD=xfcalendar
         VERSION="svn-`date +%U`"
          SOURCE=$MODULE.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE
      SOURCE_URL=svn+http://svn.xfce.org/svn/xfce/$MOD/trunk:$MOD
