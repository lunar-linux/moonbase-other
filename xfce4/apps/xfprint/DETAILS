SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE
      SOURCE_URL=http://www.xfce.org/archive/xfce-$VERSION/src/
   MODULE_PREFIX=${XFCE4_PREFIX:-/usr}
