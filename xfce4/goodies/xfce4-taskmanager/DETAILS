          MODULE=xfce4-taskmanager
         VERSION="svn-`date +%U`"
          SOURCE=$MODULE-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE
      SOURCE_URL=svn+http://svn.xfce.org/svn/goodies/$MODULE/trunk:$MODULE
   MODULE_PREFIX=${XFCE4_PREFIX:-/usr}
        WEB_SITE=http://www.xfce.org
         ENTERED=20061124
         UPDATED=20071207
           SHORT="taskmanager for the Xfce Desktop Environment"

cat << EOF
Xfce4-taskmanager is a simple taskmanager for the Xfce Desktop 
Environment.
EOF
