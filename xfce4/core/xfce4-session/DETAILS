          MODULE=xfce4-session
         VERSION="git-`date +%Y%U`"
          SOURCE=$MODULE.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE
      SOURCE_URL=git+http://git.xfce.org/xfce/$MODULE:$MODULE
   MODULE_PREFIX=${XFCE4_PREFIX:-/usr}
        WEB_SITE=http://www.xfce.org
         ENTERED=20030715
         UPDATED=20071204
           SHORT="Session manager for Xfce4"
PSAFE=no
cat << EOF
xfce4-session -- Session manager for the Xfce4 desktop environment.
Restores applications that were running in the previous session.
EOF
