          MODULE=xfwm4
         VERSION=4.6.2
          SOURCE=$MODULE-$VERSION.tar.bz2
     SOURCE_URL=http://www.xfce.org/archive/xfce-$VERSION/src
     SOURCE_VFY=sha1:34514db4531e95a7dbcd7d73c8d97e5cb9d1bf89
   MODULE_PREFIX=${XFCE4_PREFIX:-/usr}
        WEB_SITE=http://www.xfce.org
         ENTERED=20030715
         UPDATED=20100524
           SHORT="Window manager for Xfce4"

cat << EOF
xfwm4 -- Window manager for the Xfce4 desktop environement. Strictly follows
the freedesktop standard on window manager hints.
EOF
