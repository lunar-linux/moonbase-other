          SOURCE=$MOD.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MOD          
      SOURCE_URL=svn+http://svn.xfce.org/svn/xfce/$MOD/trunk:$MOD
   MODULE_PREFIX=${XFCE4_PREFIX:-/usr}
