          MODULE=libxfcegui4
         VERSION="svn-`date +%U`"
          SOURCE=$MODULE.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE
      SOURCE_URL=http:vn+http://svn.xfce.org/svn/xfce/$MODULE/trunk:$MODULE
   MODULE_PREFIX=${XFCE4_PREFIX:-/usr}
        WEB_SITE=http://www.xfce.org
         ENTERED=20030715
