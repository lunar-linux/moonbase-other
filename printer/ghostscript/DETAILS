          MODULE=ghostscript
         VERSION=8.63
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$SFORGE_URL/$MODULE
      SOURCE_VFY=sha1:c97159d3cccb0b1b5fc409715c6321a7256717ec
