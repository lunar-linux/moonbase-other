          MODULE=ghostscript
         VERSION=8.71
          SOURCE=$MODULE-$VERSION.tar.gz
         SOURCE2=$MODULE-8.64-jasper.patch
      SOURCE_URL=$SFORGE_URL/$MODULE
     SOURCE2_URL=$PATCH_URL
      SOURCE_VFY=sha1:629299140f612fac32f6289be0904107dfd1b555
