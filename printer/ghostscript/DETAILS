          MODULE=ghostscript
         VERSION=8.61
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$SFORGE_URL/$MODULE
      SOURCE_VFY=sha1:b05ac61885c0370e432531ffb79de349c6e8b896
