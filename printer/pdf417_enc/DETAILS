          MODULE=pdf417_enc
         VERSION=4.4
          SOURCE=$MODULE.$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE.$VERSION
      SOURCE_URL=$SFORGE_URL/pdf417encode/
      SOURCE_VFY=sha1:fe907b3c8784a8f4e780334d70273a6f9c6ac087
        WEB_SITE=http://webpages.charter.net/jtlien/pdfdoc.html
         ENTERED=20040111
         UPDATED=20070930
      MAINTAINER=florin@lunar-linux.org
           SHORT="An encode program for pdf417 barcode format"

cat << EOF
The pdf417_encode program will take an input text file and convert
it to postscript output in pdf417 barcode format.
EOF
