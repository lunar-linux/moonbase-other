          MODULE=dosbox
         VERSION=0.73
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE
      SOURCE_VFY=sha1:e87682f8f468f3822d2f79e38e5ddb73825fa8c1
        WEB_SITE=http://dosbox.sourceforge.net
         ENTERED=20040331
