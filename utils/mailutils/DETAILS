          MODULE=mailutils
         VERSION=3.9
          SOURCE=${MODULE}-${VERSION}.tar.xz
      SOURCE_URL=ftp://ftp.gnu.org/pub/gnu/mailutils/
      SOURCE_VFY=sha256:5f736c63b4976c94eaeddadb94d9c257760e6cb2a9fa189cd59600aaa8703dbc
