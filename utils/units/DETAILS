         VERSION=2.10
          SOURCE=$MODULE-$VERSION.tar.gz
   SOURCE_URL[0]=$GNU_URL/$MODULE
   SOURCE_URL[1]=http://www.cam.cornell.edu/~adrian
      SOURCE_VFY=sha1:9e9a8f7b5c67bd67f541bc9f78ab127ef4457297
