mquery USE_DUMB   "Enable dumb UPS driver?" y " --enable-dumb" " --disable-dumb"
mquery USE_APC    "Enable APC SmartUPS serial driver?" n " --enable-apcsmart" " --disable-apcsmart"
mquery USE_USB    "Enable USB UPS driver?" y " --enable-usb" " --disable-usb"
mquery USE_NET    "Enable networking driver for slaves ?" n " --enable-net" " --disable-net"
mquery USE_SNMP   "Enable SNMP driver?" n " --enable-snmp" " --disable-snmp"
mquery USE_TEST   "Enable test driver?" n " --enable-test" " --disable-test"
mquery USE_PCNET  "Enable PCNet driver?" n " --enable-pcnet" " --disable-pcnet"
mquery USE_MODBUS "Enable MODBUS driver?" n " --enable-modbus" " --disable-modbus"
mquery USE_MODUSB "Enable MODBUS/USB driver?" n " --enable-modbus-usb" " --disable-modbus-usb"
