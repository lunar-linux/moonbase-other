           MODULE=mknbi
         VERSION=1.2
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=$SFORGE_URL/etherboot/$SOURCE
        WEB_SITE=http://sourceforge.net/projects/etherboot/
         ENTERED=20011016
         UPDATED=20011016
           SHORT="mknbi creates images for network booting via etherboot"
cat << EOF
mknbi creates images for network booting via etherboot.
These are used to boot a diskless x86 PC over protocols like
bootp/arp/dhcp and tftp.
EOF
