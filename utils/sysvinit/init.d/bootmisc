#!/bin/sh
#
# bootmisc - system startup script
#

case $1 in
  start)
    dmesg > /var/log/dmesg
	dmesg -n4
    touch /var/run/utmp /var/run/wtmp
    [ ! -f /etc/LOCALE ] || . /etc/LOCALE
    [ ! -f /etc/keymap ] || loadkeys $(cat /etc/keymap)
    [ ! -f /etc/consolefont ] || consolechars -f $(cat /etc/consolefont)
    ;;
esac

