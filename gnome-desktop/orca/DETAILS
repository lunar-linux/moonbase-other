        MODULE=orca
       VERSION=1.0.0
         MAJOR=1.0
        SOURCE=$MODULE-$VERSION.tar.bz2
    SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
    SOURCE_VFY=sha1:5bef34ad55e259cd4ebe5a47d18a22a3f096e0b4 
       ENTERED=20060908
