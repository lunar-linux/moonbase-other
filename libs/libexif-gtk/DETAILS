          SOURCE=$MODULE-$VERSION.tar.bz2
         SOURCE2=libexif-gtk-0.3.5-libexif-0.6.13.patch
      SOURCE_URL=$SFORGE_URL/libexif/
     SOURCE2_URL=$PATCH_URL
