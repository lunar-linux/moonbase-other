          MODULE=libedit
         VERSION=20230828-3.1
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=https://thrysoee.dk/editline/
      SOURCE_VFY=sha256:4ee8182b6e569290e7d1f44f0f78dac8716b35f656b76528f699c69c98814dad
