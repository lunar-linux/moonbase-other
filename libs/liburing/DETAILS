          MODULE=liburing
         VERSION=2.14
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=https://github.com/axboe/liburing/archive/refs/tags
      SOURCE_VFY=sha256:5f80964108981c6ad979c735f0b4877d5f49914c2a062f8e88282f26bf61de0c
