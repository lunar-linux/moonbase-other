          SOURCE=$MODULE-$VERSION.tar.gz
         SOURCE2=$MODULE-$VERSION-raise_cap.patch.bz2
      SOURCE_URL=$KERNEL_URL/pub/linux/libs/security/linux-privs/libcap2
