depends cmake
depends x265
depends %JPEG
depends libpng
depends libde265

optional_depends aom     "-DWITH_AOM_DECODER=ON" "-DWITH_AOM_DECODER=OFF" "for AV1 Codec support"
optional_depends rav1e   "-DWITH_RAV1E=ON"       "-DWITH_RAV1E=OFF"       " for AV1 encoder supoort"
optional_depends SVT-AV1 "-DWITH_SvtEnc=ON"      "-DWITH_SvtEnc=OFF"      "for video scaling support"
