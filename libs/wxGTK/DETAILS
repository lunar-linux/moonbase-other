          SOURCE=wxWidgets-$VERSION.tar.bz2
      SOURCE_URL=https://github.com/wxWidgets/wxWidgets/releases/download/v${VERSION}/
      SOURCE_VFY=sha256:dffcb6be71296fff4b7f8840eb1b510178f57aa2eb236b20da41182009242c02
