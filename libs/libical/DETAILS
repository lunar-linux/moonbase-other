          MODULE=libical
         VERSION=0.23
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=http://softwarestudio.org/download/$SOURCE
        WEB_SITE=http://www.softwarestudio.org/libical
