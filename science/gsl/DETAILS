          MODULE=gsl
         VERSION=1.8
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$GNU_URL/$MODULE/
      SOURCE_VFY=sha1:0dabdf2d024ae3b29207e485c5413676f3fa136f
        WEB_SITE=http://www.gnu.org/software/gsl/
