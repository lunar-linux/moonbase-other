          MODULE=libqalculate
         VERSION=0.9.3
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/qalculate/
      SOURCE_VFY=sha1:4944811a1bfb2f49d3dc59386dcd2aa740fc2ac1
        WEB_SITE=http://qalculate.sf.net
