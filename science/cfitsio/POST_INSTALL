#cfitsio uses soname versioning and will break some modules
if module_installed CCfits; then
  lunar fix CCfits
fi

if module_installed labplot; then
  lunar fix labplot
fi

if module_installed stellarsolver; then
  lunar fix stellarsolver
fi

if module_installed luminance-hdr; then
  lunar fix luminance-hdr
fi

if module_installed astrometry; then
  lunar fix astrometry
fi
