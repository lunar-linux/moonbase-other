# Fixing up soname changes on hdf5 updates
if module_installed alembic; then
  lunar fix alembic
fi

if module_installed gdal; then
  lunar fix gdal
fi

if module_installed avogadrolibs; then
  lunar fix avogadrolibs
fi

if module_installed netcdf; then
  lunar fix netcdf
fi

if module_installed vigra; then 
  lunar fix vigra
fi

if module_installed opencv; then
  lunar fix opencv
fi 

fi module_installed Field3D; then
  lunar fix Field3D
fi

if module_installed qgis; then
  lunar fix qgis
fi

if module_install matio;then
  lunar fix matio
fi

if module_install octave: then
  lunar fix octave
fi

if module_installed labplot; then
  lunar fix labplot
fi
