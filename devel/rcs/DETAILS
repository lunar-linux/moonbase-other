          MODULE=rcs
         VERSION=5.10.1
          SOURCE=$MODULE-$VERSION.tar.lz
   SOURCE_URL[0]=$GNU_URL/$MODULE
   SOURCE_URL[1]=https://ftp.gnu.org/pub/gnu/$MODULE
      SOURCE_VFY=sha256:43ddfe10724a8b85e2468f6403b6000737186f01e60e0bd62fde69d842234cc5
