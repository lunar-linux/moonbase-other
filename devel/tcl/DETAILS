            MODULE=tcl
           VERSION=8.6.13
            SOURCE=$MODULE$VERSION-src.tar.gz
  SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE$VERSION
        SOURCE_URL=$SFORGE_URL/$MODULE
       SOURCE_VFY=sha256:43a1fae7412f61ff11de2cfd05d28cfc3a73762f354a417c62370a54e2caf066
          WEB_SITE=https://tcl.sourceforge.net
           ENTERED=20010922
