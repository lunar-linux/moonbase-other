        MODULE=swig
       VERSION=4.1.1
        SOURCE=$MODULE-$VERSION.tar.gz
    SOURCE_URL=https://downloads.sourceforge.net/${MODULE}/
    SOURCE_VFY=sha256:2af08aced8fcd65cdb5cc62426768914bedc735b1c250325203716f78e39ac9b
      WEB_SITE=http://www.swig.org
       ENTERED=20011207
