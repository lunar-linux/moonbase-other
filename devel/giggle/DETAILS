          MODULE=giggle
           MAJOR=0.5
         VERSION=${MAJOR}
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$GNOME_URL/sources/$MODULE/$MAJOR
      SOURCE_VFY=sha1:43143e49be8f510e126c19c68ce19870448e9430
        WEB_SITE=http://live.gnome.org/giggle
         ENTERED=20070427
         UPDATED=20100603
           SHORT="gtk+ frontend to git"

cat << EOF
Giggle is a graphical frontend for the git directory tracker (think of
gitk on GTK+).
EOF
