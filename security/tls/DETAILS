          MODULE=tls
         VERSION=1.5.0
          SOURCE=$MODULE$VERSION-src.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE/
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE\1.5
      SOURCE_VFY=sha1:66611f384f4b1c5e126ac8fc1cd03040bbe8ac16
