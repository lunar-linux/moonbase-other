          MODULE=clojurescript
         VERSION=1.8.51
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-r$VERSION
 SOURCE_URL_FULL=http://github.com/clojure/clojurescript/archive/r$VERSION.tar.gz
