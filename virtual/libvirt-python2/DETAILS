          MODULE=libvirt-python2
         VERSION=5.9.0
          SOURCE=libvirt-python-$VERSION.tar.gz
      SOURCE_URL=http://libvirt.org/sources/python
      SOURCE_VFY=sha256:
SOURCE_DIRECTORY=$BUILD_DIRECTORY/libvirt-python-$VERSION
        WEB_SITE=http://libvirt.org/
            TYPE=python2
         ENTERED=20140920
         UPDATED=20200207
           SHORT="libvirt python binding"

