          MODULE=libvirt
         VERSION=1.1.3
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=http://libvirt.org/sources
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
      SOURCE_VFY=sha256:
        WEB_SITE=http://libvirt.org
         ENTERED=20130202
         UPDATED=20131001
           SHORT="API for controlling virtualization engines"

cat <<EOF
