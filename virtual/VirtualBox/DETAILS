          MODULE=VirtualBox
         VERSION=4.0.12
          SOURCE=$MODULE-$VERSION.tar.bz2
         SOURCE2=$MODULE-3.2.0-gcc-4.5-warning-workaround.patch.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-${VERSION}_OSE
