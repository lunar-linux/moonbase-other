      SOURCE_URL=$SFORGE_URL/darktable/darktable/${VERSION%.*}/
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
      SOURCE_VFY=sha1:d007ada0f3fb3af6861de74323577b90eec3996e
