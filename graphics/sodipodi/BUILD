(
 ./configure --prefix=/usr  \
                --localstatedir=/var  \
                --sysconfdir=/etc  \
                --libdir=/usr/lib  \
                --includedir=/usr/include  \
                --infodir=/usr/share/info  \
                --mandir=/usr/share/man  \
                --without-bonobo        &&
 default_make
) > $C_FIFO 2>&1

