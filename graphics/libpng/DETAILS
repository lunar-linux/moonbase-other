          MODULE=libpng
         VERSION=1.6.44
          SOURCE=$MODULE-$VERSION.tar.xz
   SOURCE_URL[0]=$SFORGE_URL/$MODULE
   SOURCE_URL[1]=https://fossies.org/linux/misc/
      SOURCE_VFY=sha256:60c4da1d5b7f0aa8d158da48e8f8afa9773c1c8baa5d21974df61f1886b8ce8e
