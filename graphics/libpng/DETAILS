          MODULE=libpng
         VERSION=1.2.35
          SOURCE=$MODULE-$VERSION.tar.bz2
         SOURCE2=libpng-1.2.35-png.h.patch
      SOURCE_URL=$SFORGE_URL/$MODULE
     SOURCE2_URL=$PATCHJ_URL
