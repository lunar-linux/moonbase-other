mquery ENBL_PRNT_PROF "Enable print profiles?" y \
                                 "-Denable-print-profiles=true" \
                                 "-Denable-print-profiles=false"

mquery ENBL_REVERSE "Enable reverse engineering tools?" n \
                                 "-Denable-reverse=true" \
                                 "-Denable-reverse=false"
                                 
mquery ENBL_ARGYLL "Enable ArgyllCMS sensor ?" n \
                                 "-Denable-argyllcms-sensor=true" \
                                 "-Denable-argyllcms-sensor=false"
