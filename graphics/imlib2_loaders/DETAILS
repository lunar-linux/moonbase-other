          MODULE=imlib2_loaders
         VERSION=1.2.1
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/enlightenment/
      SOURCE_VFY=sha1:2d203d8c8f907ce9ec0a5dffe58d73be68e88a19
        WEB_SITE=http://enlightenment.org
