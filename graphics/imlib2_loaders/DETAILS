          MODULE=imlib2_loaders
         VERSION=1.2.2
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/enlightenment/
      SOURCE_VFY=sha1:481d29092dadf9c2d6d7beda08d0e34952f7b994
        WEB_SITE=http://enlightenment.org
