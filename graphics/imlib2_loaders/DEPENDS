depends  imlib2

optional_depends  "edb"  "--enable-edb"   \
                         "--disable-edb"  \
                         "to build the edb loader"

optional_depends  "eet"  "--enable-eet --with-eet-config=/usr/lib/pkgconfig/eet.pc"   \
                         "--disable-eet"  \
                         "to build the eet loader"
