mquery ENABLE_THREADS "Enable threading support?" y "--enable-threads" "--disable-threads"
mquery INST_DATA_FILES "Install configuration and data files?" y "--enable-datafiles" "--disable-datafiles"
mquery ENABLE_ESTI "Enable ETSI AT modem support?" y "--enable-atmodem" "--disable-atmodem"
mquery ENABLE_CDMA "Enable CDMA AT modem support?" y "--enable-cdmamodem" "--disable-cdmamodem"
mquery ENABLE_SIM "Enable Phone simulator support?" y "--enable-phonesim" "--disable-phonesim"
mquery ENABLE_ISI "Enable PhoNet/ISI modem support?" y "--enable-isimodem" "--disable-isimodem"
