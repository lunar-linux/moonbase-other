optional_depends "ruby"   "-DENABLE_RUBY=YES"   "-DENABLE_RUBY=NO"   "for ruby bindings support"
optional_depends "tcl"    "-DENABLE_TCL=YES"    "-DENABLE_TCL=NO"    "for tcl support"
