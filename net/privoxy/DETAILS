SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION-stable
      SOURCE_URL=http://downloads.sourceforge.net/ijbswa/
      SOURCE_VFY=sha256:04b104e70dac61561b9dd110684b250fafc8c13dbe437a60fae18ddd9a881fae
