          MODULE=dhcp
         VERSION=4.4.3
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL[0]=ftp://ftp.isc.org/isc/$MODULE/$VERSION
      SOURCE_URL[1]=https//ftp.isc.org/isc/$MODULE/$VERSION
      SOURCE_VFY=sha256:0e3ec6b4c2a05ec0148874bcd999a66d05518378d77421f607fb0bc9d0135818
