          MODULE=xvinfo
         VERSION=1.1.0
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$XORG_URL/individual/app
      SOURCE_VFY=sha1:8695bfb597dbf3ed8e3277fddda4722da21baaa3
   MODULE_PREFIX=${X11R7_PREFIX:-/usr}
        WEB_SITE=http://www.x.org
         ENTERED=20060302
         UPDATED=20091118
           SHORT="Print out X-Video extension adaptor information"

cat << EOF
xvinfo prints out the capabilities of any video adaptors associated with
the display that are accesible through the X-Video extension.
EOF
