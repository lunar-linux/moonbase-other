          MODULE=glproto
         VERSION=1.4.10
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$XORG_URL/individual/proto
      SOURCE_VFY=sha1:30d312863288316da26aa33bfd7d0b303c8b0851
   MODULE_PREFIX=${X11R7_PREFIX:-/usr}
        WEB_SITE=http://www.x.org
         ENTERED=20060120
