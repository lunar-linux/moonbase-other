          MODULE=glproto
         VERSION=1.4.9
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$XORG_URL/individual/proto/
      SOURCE_VFY=sha1:6a8e17ff3ac08224d0aef2b3c27b6aecfebaa95b
