          MODULE=evieext
         VERSION=1.1.0
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$XORG_URL/individual/proto
      SOURCE_VFY=sha1:d0b57a8fbbf0845588a690542a6c29d351c2b4e5
   MODULE_PREFIX=${X11R7_PREFIX:-/usr}
        WEB_SITE=http://www.x.org
         ENTERED=20060120
         UPDATED=20090827
           SHORT="the X.Org EvIE protocol headers"

cat << EOF
evieext provides the wire protocol for the EvIE extension in X.Org.
It is used to let arbitrary clients receive, filter, and potentially
modify all events.
EOF
