          MODULE=xf86-video-nsc
         VERSION=X11R7.2-2.8.2
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$XORG_URL/X11R7.2/src/driver/
      SOURCE_VFY=sha1:f4120fcf9a187f6e28f5d09271ac51e010c6600c
   MODULE_PREFIX=${X11R7_PREFIX:-/usr}
        WEB_SITE=http://www.x.org/
         ENTERED=20060303
         UPDATED=20070216
           SHORT="the X.Org video driver for NSC Geode based cards"

cat << EOF
xf86-video-nsc provides the X.Org video driver for the National 
Semiconductors GEODE processor based video cards. It uses the DURANGO
kit provided by National Semiconductor.  The driver is accelerated, and
provides support for the following framebuffer depths: 8, 16 and 24.
The nsc driver supports GXLV (5530 companion chip), SC1200, SC1400 and 
GX2 (5535 companion chip).
EOF
