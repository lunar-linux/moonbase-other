          MODULE=libXaw
         VERSION=1.0.4
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$XORG_URL/individual/lib
      SOURCE_VFY=sha1:9bf93239e6329ec819b6bfd00455cc0efffac103
   MODULE_PREFIX=${X11R7_PREFIX:-/usr}
