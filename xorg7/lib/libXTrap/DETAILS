          MODULE=libXTrap
         VERSION=1.0.0
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$XORG_URL/individual/lib/
      SOURCE_VFY=sha1:3ed2d68e7bfcb065cd6e9d6b1e38044f2fb355c9
