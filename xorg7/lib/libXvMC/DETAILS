          MODULE=libXvMC
         VERSION=1.0.5
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$XORG_URL/individual/lib/
      SOURCE_VFY=sha1:153b85884f22b882cecb9fc462fe24c669a80dbd
   MODULE_PREFIX=${X11R7_PREFIX:-/usr}
        WEB_SITE=http://www.x.org/
         ENTERED=20060120
         UPDATED=20091120
           SHORT="the X.Org XvMC extension library"
           PSAFE=no

cat << EOF
libXvMC provides access to the XVideo-MotionCompensation (XvMC) extension
for the X11 protocol.  This provides montion compensation support to the
X11 XVideo extension.
EOF
