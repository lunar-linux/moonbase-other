          MODULE=libXfontcache
         VERSION=1.0.4
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$XORG_URL/individual/lib/
      SOURCE_VFY=sha1:25fbf6df365c0296b58124c46647facaf9072668
   MODULE_PREFIX=${X11R7_PREFIX:-/usr}
        WEB_SITE=http://www.x.org/
         ENTERED=20060120
         UPDATED=20070216
           SHORT="the X-TrueType font cache extension client library"

cat << EOF
libXfontcache is the X-TrueType font cache extension client library.
FontCache is an extension that is used by X-TrueType to cache
information about fonts.
EOF
