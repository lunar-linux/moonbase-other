          MODULE=util-macros
         VERSION=1.14.0
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$XORG_URL/individual/util
      SOURCE_VFY=sha1:e9863638cc9119a7b8d77b554de30a99a3611075
   MODULE_PREFIX=${X11R7_PREFIX:-/usr}
        WEB_SITE=http://www.x.org
         ENTERED=20070216
