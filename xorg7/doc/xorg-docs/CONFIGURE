mquery ENABLE_PS "Build PostScript versions of documents" n "--enable-ps" "--disable-ps"  &&
mquery ENABLE_PDF "Build PDF versions of documents" n "--enable-pdf" "--disable-pdf" &&
mquery ENABLE_TXT "Build txt versions of documents" n "--enable-txt" "--disable-txt" &&
mquery ENABLE_HTML "Build HTML versions of documents" n "--enable-html" "--disable-html" 

