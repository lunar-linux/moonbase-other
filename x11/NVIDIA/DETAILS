          MODULE=NVIDIA
         VERSION=1.0-8774
          SOURCE=$MODULE-Linux-x86-$VERSION-pkg1.run
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-Linux-x86-$VERSION-pkg1
      SOURCE_URL=http://download.nvidia.com/XFree86/Linux-x86/$VERSION/
      SOURCE_VFY=sha1:fcd38bad3240b6b24cfef278238014351af98062
        WEB_SITE=http://www.nvidia.com
