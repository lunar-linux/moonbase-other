# Every time you update ktorrent remember to also update libktorrent.
          MODULE=ktorrent
           MAJOR=4.1
         VERSION=$MAJOR.1
          SOURCE=ktorrent-$VERSION.tar.bz2
      SOURCE_URL=http://ktorrent.org/downloads/$VERSION
   MODULE_PREFIX=${KDE4_INSTALL_DIR:-/usr}
