          MODULE=kdenetwork
         VERSION=4.8.0
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$KDE_URL/stable/$VERSION/src
      SOURCE_VFY=sha1:51ea55dbcbece59535af61f7450e8ac2aa1db28b
   MODULE_PREFIX=${KDE4_INSTALL_DIR:-/usr}
        WEB_SITE=http://www.kde.org
         ENTERED=20071026
         UPDATED=20120126
           SHORT="Network tools for KDE"

cat << EOF
* Filesharing: network file sharing
* kdnssd: Zeroconf
* KGet: an advanced download manager
* KNewsTicker: RDF newsticker Plasma applet
* Kopete: Instant Messaging client
* KPPP: dialer and front end for pppd
* KRDC: a client for Desktop Sharing
* Krfb: Desktop Sharing server, allow others to access your desktop
  via VNC
* Lanbrowsing: lan browsing kio slave.
EOF
