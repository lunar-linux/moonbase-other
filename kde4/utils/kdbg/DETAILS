          MODULE=kdbg
           MAJOR=2.5
         VERSION=${MAJOR}.0
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE/Source%20Code/$VERSION/
