          MODULE=bchunk
         VERSION=1.2.0
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=http://he.fi/$MODULE
      SOURCE_VFY=sha1:a328e4665eb5e51df63d8d27d1d75ecc74bdef9e
