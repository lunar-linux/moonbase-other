depends  gtk+-2
depends  libcddb
depends  libcdio

optional_depends  "libvorbis"  ""  "--disable-vorbis"  \
                  "support for ripping to Ogg"

optional_depends  "lame"       ""  "--disable-mp3"     \
                 "support for ripping to MP3"

optional_depends  "flac"       ""  "--disable-flac"    \
                 "support for ripping to FLAC"
