          MODULE=libcdio
         VERSION=2.3.0
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=https://github.com/libcdio/libcdio/releases/download/$VERSION/
      SOURCE_VFY=sha256:53e83d284667535a767fd2d31edad1a6701591960459df373a10f1f21e80a7ed
