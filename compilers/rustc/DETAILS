          MODULE=rustc
         VERSION=1.91.1
          SOURCE=$MODULE-$VERSION-src.tar.xz
      SOURCE_URL=https://static.rust-lang.org/dist/
      SOURCE_VFY=sha256:66401bb815e236cc6b2aacbbe23b61b286c1fe27a67902e7c0222cfe77b3dbab
