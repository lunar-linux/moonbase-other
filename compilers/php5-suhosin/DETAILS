     SOURCE2_URL=http://download.suhosin.org
     SOURCE3_URL=http://download.suhosin.org
      SOURCE_VFY=sha1:fe0626735c3d9dd370cef9bdcfe9506629449f51
     SOURCE2_VFY=sha1:abb30c22e7fe341955b42ec71ed597c43439e2b8
