          MODULE=lsdvd
         VERSION=0.16
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE/
      SOURCE_VFY=sha1:d5c0d32bfb220807ebdc0bfbb17679e7294791f4
         SOURCE2=$MODULE-$VERSION-types.patch
     SOURCE2_URL=$PATCH_URL
