         MODULE=libdvdread
        VERSION=0.9.6
         SOURCE=$MODULE-$VERSION.tar.gz
        SOURCE2=$MODULE-$VERSION-udfsymbols.patch
     SOURCE_URL=http://www.dtek.chalmers.se/groups/dvd/dist/
