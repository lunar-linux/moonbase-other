          MODULE=sysvinit
         VERSION=3.04
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=https://download.savannah.gnu.org/releases/$MODULE
      SOURCE_VFY=sha256:2a621fe6e4528bc91308b74867ddaaebbdf7753f02395c0c5bae817bd2b7e3a5
