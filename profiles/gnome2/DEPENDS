if  ! grep -q "METACITY"  $MODULE_CONFIG; then
  if query "Install metacity instead of sawfish?" y ; then 
    depends metacity
    echo  "METACITY=y"  >>  $MODULE_CONFIG
  else
    depends sawfish
    echo  "METACITY=n"  >>  $MODULE_CONFIG
  fi
fi                              &&

