          MODULE=cwtext
         VERSION=0.96
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE/
      SOURCE_VFY=sha1:5e6d7d57b5329de80fce84eb65d47c0b776f2d4f
        WEB_SITE=http://cwtext.sourceforge.net/index.php
         ENTERED=20011212
         UPDATED=20090825
           SHORT="Converts text to morse code"

cat << EOF
cwtext converts plain ASCII to International Morse Code.
It includes three programs,
 cwtext: convert text to 'dotscii' - . -..- -
  cwpcm: convert text to raw 8bit PCM audio stream
   cwmm: convert text to MorseMail XML format
EOF
