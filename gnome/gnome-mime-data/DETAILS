           MODULE=gnome-mime-data
         VERSION=1.0.1
          SOURCE=$MODULE-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=$GNOME_URL/stable/sources/$MODULE/$SOURCE
   SOURCE_URL[1]=ftp://ftp.gnome.org/pub/GNOME/stable/sources/$MODULE/$SOURCE
        WEB_SITE=http://www.gnome.org
         ENTERED=20020124
         UPDATED=20020124
           SHORT="gnome-mime-data contains the base MIME/app database for GNOME"
cat << EOF
This module contains the base MIME and Application database for GNOME.
It is meant to be accessed through the MIME functions in GnomeVFS.
EOF
