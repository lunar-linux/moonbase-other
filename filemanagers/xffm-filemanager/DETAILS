          MODULE=xffm-filemanager
         VERSION=4.5.0
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/xffm/
      SOURCE_VFY=sha1:685ab266ebf95b646e67029267ff981420873584
        WEB_SITE=http://xffm.org
         ENTERED=20070410
         UPDATED=20070410
           SHORT="File manager for Xffm"

cat << EOF
xffm -- File manager for the Xffm Code Set.
Previously Xffm was only a filemanager. Now Xffm is not a filemanager but a
whole set of applications and libraries, (among which is the totally rebuild
xffm-filemanager, which is still not your usual run-of-the-mill filemanager).
EOF
