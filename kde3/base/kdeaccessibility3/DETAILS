          MODULE=kdeaccessibility3
         VERSION=3.5.6
          SOURCE=kdeaccessibility-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/kdeaccessibility-$VERSION
      SOURCE_URL=$KDE_URL/stable/$VERSION/src/
   MODULE_PREFIX=${KDE_PREFIX:-/opt/lunar/kde/3}
      SOURCE_VFY=sha1:d39335fb302cb4180500590ad70041a61250559d
        WEB_SITE=http://www.kde.org/
         ENTERED=20040103
         UPDATED=20070126
      MAINTAINER=florin@lunar-linux.org
      MAINTAINER=jol@lunar-linux.org
           SHORT="Contains accessibility apps for KDE"

cat << EOF
kdeaccessibility contains accessibility apps for KDE:
kmag, a screen magnifier, kmousetool, a program for people whom it 
hurts to click the mouse, and KMouth, a program that allows people 
who have lost their voice to let their computer speak for them.
EOF
