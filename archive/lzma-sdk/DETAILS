       MODULE=lzma-sdk
      VERSION=465
       SOURCE=lzma$VERSION.tar.bz2
SOURCE_URL[0]=$SFORGE_URL/sevenzip/
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_VFY=sha1:5ec1c4606fec88c770a9712073e83916f8aed173
     WEB_SITE=http://www.7-zip.org/
