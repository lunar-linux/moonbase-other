       MODULE=lzma-sdk
      VERSION=2301
       SOURCE=lzma$VERSION.7z
SOURCE_URL[0]=$SFORGE_URL/sevenzip/
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_VFY=sha256:317dd834d6bbfd95433488b832e823cd3d4d420101436422c03af88507dd1370
     WEB_SITE=http://www.7-zip.org/
      ENTERED=20080129
