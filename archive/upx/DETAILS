         VERSION=3.04-src
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=http://upx.sourceforge.net/download
      SOURCE_VFY=sha1:76e509efb52e1b891e929601f7cb8acbe349445e
        WEB_SITE=http://upx.sourceforge.net
         ENTERED=20021220
