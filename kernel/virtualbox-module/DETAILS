          MODULE=virtualbox-module
         VERSION=`module_version VirtualBox`
          SOURCE=VirtualBox-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/VirtualBox-$VERSION
      SOURCE_URL=http://download.virtualbox.org/virtualbox/$VERSION
        WEB_SITE=http://virtualbox.org
         ENTERED=20090202
         UPDATED=20110501
           SHORT="Kernel module for VirtualBox"

cat << EOF
Sources to the kernel-level portion of VirtualBox.
EOF
