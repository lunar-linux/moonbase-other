          MODULE=virtualbox-module
         VERSION=`module_version VirtualBox`
          SOURCE=$MODULE-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/src/
        WEB_SITE=http://virtualbox.org
         ENTERED=20090202
