kreqs="alsa-driver bluez-kernel cipe cryptoapi cvsfs ftpfs hostap-driver iptables ipw2100 kernel-reqs ltmodem madwifi mwavem nforce NVIDIA openafs-driver pcmcia-cs snare-core submount"

for req in $kreqs; do
  if module_installed $req ; then
    # ignore failures as much as possible
    lin -c $req || true
  fi
done
