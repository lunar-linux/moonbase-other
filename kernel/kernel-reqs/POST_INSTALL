kreqs="alsa-driver bluez-kernel cipe cryptoapi cvsfs ftpfs hostap-driver iptables ipw2100 ltmodem madwifi mwavem NFORCE NVIDIA openafs-driver pcmcia-cs slmodem snare-core submount"

for req in $kreqs; do
  if module_installed $req ; then
    # ignore failures as much as possible
    lin -c $req || true
  fi
done
