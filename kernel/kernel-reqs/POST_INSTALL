kreqs="alsa-driver bluez-kernel cipe cryptoapi cvsfs ftpfs hostap-driver iptables ipw2100 ipw2200 ndiswrapper ltmodem madwifi-cvs mwavem NFORCE NVIDIA NVIDIA-old openafs-driver pcmcia-cs slmodem snare-core submount"

for req in $kreqs; do
  if module_installed $req ; then
    # ignore failures as much as possible
    lin -c $req || true
  fi
done
