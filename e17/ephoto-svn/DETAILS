          MODULE=ephoto-svn
         VERSION=svn
          SOURCE=$MODULE.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE
      SOURCE_URL=svn+http://svn.enlightenment.org/svn/e/trunk/ephoto
        WEB_SITE=http://www.enlightenment.org
         ENTERED=20070409
         UPDATED=`date --date="last monday" +%Y%m%d`
   MODULE_PREFIX=${E17_PREFIX:-/opt/lunar/enlightenment}
           SHORT="an implementation of freedesktop.org specifications"

cat << EOF
